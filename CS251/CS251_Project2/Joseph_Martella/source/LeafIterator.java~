private class LeafIterator implements Iterator<Item> {
    private int i = items.length;
    
    for (int j = i; j > 0; j--) {
        if (items[j] != null) 
            break;
        i = j;
    }
    
    public boolean hasNext() { return i > 0;                     }
    public void remove()     { /* not implemented, do not use */ }
    public Item next()       { return items[--i];                }
}